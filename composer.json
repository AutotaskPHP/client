{
    "name": "autotask/client",
    "description": "An HTTP client for interacting with Autotask APIs.",
    "license": "MIT",
    "authors": [
        {
            "name": "Aidan Casey",
            "email": "aidan.casey@anteris.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Autotask\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Autotask\\Tests\\Client\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "psr/http-message": "^1.0.1",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-client-implementation": "^1.0",
        "psr/http-message-implementation": "^1.0",
        "psr/http-factory-implementation": "^1.0",
        "illuminate/collections": "^9.48",
        "php-http/discovery": "^1.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.28",
        "aidan-casey/mock-client": "dev-master",
        "laminas/laminas-diactoros": "^2.24",
        "guzzlehttp/guzzle": "^7.5",
        "vimeo/psalm": "5.6.0",
        "nikic/php-parser": "^v4.13.2"
    },
    "scripts": {
        "coverage": "vendor/bin/phpunit --coverage-html ./build/coverage",
        "psalm": "vendor/bin/psalm --no-cache",
        "test": "vendor/bin/phpunit"
    }
}
